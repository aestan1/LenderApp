<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.estan.lenderapp">
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/la_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/la_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Principal"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AgregarCliente" />
        <activity android:name=".Simulador" />
        <activity android:name=".AgregarPrestamo" />
        <activity android:name=".AgregarAbono" />

        <activity android:name=".ReporteClientes" />
        <activity android:name=".TarjetaIndividual" />
        <activity android:name=".VerCliente"></activity>
    </application>

</manifest>